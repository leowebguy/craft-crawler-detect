{
  "name": "leowebguy/craft-crawler-detect",
  "description": "Craft plugin for detecting bots/crawlers/spiders",
  "keywords": [
    "crawler",
    "detect",
    "bots",
    "spiders"
  ],
  "version": "1.0.3",
  "type": "craft-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Leo",
      "homepage": "https://github.com/leowebguy"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "^3.0|^4.0",
    "jaybizzle/crawler-detect": "1.*"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "leowebguy\\crawlerdetect\\": "src/"
    }
  },
  "extra": {
    "name": "Crawler Detect",
    "handle": "crawler-detect",
    "hasCpSection": false,
    "hasCpSettings": false,
    "class": "leowebguy\\crawlerdetect\\CrawlerDetect",
    "components": {
      "crawlerDetectService": "leowebguy\\crawlerdetect\\services\\CrawlerDetectService"
    }
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "check-cs": "vendor/bin/ecs check src --ansi",
    "fix-cs": "vendor/bin/ecs check src --ansi --fix",
    "phpstan": "vendor/bin/phpstan analyse src"
  }
}
